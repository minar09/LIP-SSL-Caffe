  bottom: "fc7_concat"
  top: "att_conv1"
  name: "att_conv1"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    kernel_size: 3
    pad: 1
  }
}
layer {
  bottom: "att_conv1"
  top: "att_conv1"
  name: "relu_att_conv1"
  type: "ReLU"
}
