### add features ###
layer {
  bottom: "fc8_product"
  bottom: "fc8_res075_product"
  bottom: "fc8_res05_product"
  top: "fc8_fusion"
  name: "fc8_fusion"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}
############### shrink label ################
layer {
  bottom: "label"
  top: "label_shrink8"
  name: "label_shrink8"
  type: "Interp"
  interp_param {
    shrink_factor: 8
    pad_beg: 0
    pad_end: 0
  }
}
layer {
  name: "label_pose_1st"
  type: "PoseEvaluate"
  bottom: "label_shrink8"
  top: "label_pose_1st"
  pose_evaluate_param {
    num_joint: 9
  }
}
layer{
  name: "label_heatmap"
  type: "PoseCreate"
  bottom: "label_pose_1st"
  bottom: "label_shrink8"
  top: "label_heatmap"
  pose_create_param {
    num_joint: 9
  }
}
############### compute loss first order#################
layer {
  name: "fc8_mask_1st"
  type: "MaskCreate"
  bottom: "fc8_fusion"
  top: "fc8_mask_1st"
  mask_create_param {
    num_cls: 20
  }
}
layer {
  name: "fc8_pose_1st"
  type: "PoseEvaluate"
  bottom: "fc8_mask_1st"
  top: "fc8_pose_1st"
  pose_evaluate_param {
    num_joint: 9
  }
}
layer {
  name: "predict_heatmap"
  type: "PoseCreate"
  bottom: "fc8_pose_1st"
  bottom: "fc8_mask_1st"
  top: "predict_heatmap"
  pose_create_param {
    num_joint: 9
  }
}
layer {
  name: "heatmap_error"
  type: "HeatmapError"
  bottom: "predict_heatmap"
  bottom: "label_heatmap"
  top: "heatmap_error"
  heatmap_error_param {
    scale: 0.05
  }
  propagate_down:false
  propagate_down:false
}
layer {
  name: "loss_first"
  type: "SoftmaxWithLoss"
  bottom: "fc8_fusion"
  bottom: "label_shrink8"
  bottom: "heatmap_error"
  top: "loss_first"
  loss_param {
    ignore_label: 255
  }
}
layer {
  name: "accuracy_first"
  type: "SegAccuracy"
  bottom: "fc8_fusion"
  bottom: "label_shrink8"
  top: "accuracy_first"
  seg_accuracy_param {
    ignore_label: 255
  }
}

